{
  "version": "1.0.0",
  "name": "nmsg",
  "description": "TCP transport for nmsg",
  "main": "dist/index.min.js",
  "keywords": [
    "nmsg",
    "nmsg-tcp",
    "tcp",
    "messenger",
    "node-messenger",
    "messenger",
    "ipc",
    "inter-process",
    "communication",
    "scoket",
    "websocket",
    "sockjs",
    "socket.io"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/streamich/nmsg-tcp"
  },
  "scripts": {
    "mypublish": "npm run typing && git add -A && git commit -m \"Publishing\" && npm version patch && git push -u origin master && npm publish",
    "typing": "node ./dts.js",
    "test": "mocha",
    "dist-build": "webpack && echo module.exports= | cat - dist/webpack.js > dist/index.js",
    "dist-min": "uglifyjs dist/index.js -o dist/index.min.js --compress --mangle",
    "dist": "npm run dist-build && npm run dist-min"
  },
  "dependencies": {
    "msgpack-lite": "0.1.17"
  }
}
